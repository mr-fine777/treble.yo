{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/upload",
      "methods": ["OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "https://treble.top",
        "Access-Control-Allow-Methods": "GET, POST, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type",
        "Access-Control-Max-Age": "86400"
      },
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
